{
  "name": "astra-client",
  "productName": "Astra Client",
  "description": "Advanced Minecraft Launcher",
  "author": "Astra Team",
  "private": true,
  "version": "1.0.4",
  "main": "dist-electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc -b --noEmit",
    "ts:build": "tsc -p electron/tsconfig.json",
    "ts:watch": "tsc -p electron/tsconfig.json -w",
    "electron": "wait-on tcp:5173 && cross-env NODE_ENV=development electron .",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"npm run ts:watch\" \"npm run electron\"",
    "electron:build": "vite build && npm run ts:build && electron-builder",
    "electron:publish": "vite build && npm run ts:build && dotenv -e .env -- electron-builder --publish always"
  },
  "dependencies": {
    "@react-three/drei": "^9.96.1",
    "@react-three/fiber": "^8.15.12",
    "@supabase/supabase-js": "^2.39.3",
    "clsx": "^2.1.1",
    "discord-rpc": "^4.0.1",
    "electron-updater": "^6.7.3",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.511.0",
    "minecraft-launcher-core": "^3.18.2",
    "msmc": "^5.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.3.0",
    "skinview3d": "^3.4.1",
    "tailwind-merge": "^3.0.2",
    "three": "^0.158.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.30",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^11.0.0",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "wait-on": "^7.2.0"
  }
}
